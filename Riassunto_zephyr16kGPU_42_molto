- 10.16.2.2 (tcp/443)
- 10.16.2.1 (tcp/443)
- 192.168.2.31 (tcp/443)
- 192.168.2.30 (tcp/443)
- 192.168.2.22 (tcp/443)
- 192.168.2.21 (tcp/443)
- 192.168.2.11 (tcp/443)
- 192.168.2.3 (tcp/443)
- 192.168.2.2 (tcp/443)
- 192.168.2.1 (tcp/443)
- 77.44.11.31 (tcp/443)
- 77.44.11.30 (tcp/443)
